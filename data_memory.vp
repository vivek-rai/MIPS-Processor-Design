//data_memory.vp
module `mname`(
  input logic write_en, read_en,
  input logic [6:0] address,
  input logic [31:0] write_data,
  output logic [31:0] data);
  logic [31:0] dm [127:0];
  always_comb
      case({write_en,read_en})
      2'b10:
        dm[address] = write_data;
      2'b01:
        data = dm[address];
    endcase
  always_comb
    $display("Data Memory Module");
endmodule